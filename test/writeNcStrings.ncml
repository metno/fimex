<?xml version="1.0" encoding="UTF-8"?>
<netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2 http://www.unidata.ucar.edu/schemas/netcdf/ncml-2.2.xsd">

<attribute type="strings" name="one_string" separator="|" value="string string" />
<!-- default separator is " \r\n\t", so "two_strings" is split at the space -->
<attribute type="strings" name="two_strings" value="string1 string2" />
<!-- test xml escaping -->
<attribute type="strings" name="evil_strings" value="string&amp;1,&quot;string2&quot;" />
</netcdf>
